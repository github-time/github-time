<!--miniprogram/custom-tab-bar/index.wxml-->
<view class="simple-tab-bar {{extClass}}">
  <view wx:for="{{list}}"
    wx:key="index"
    class="simple-tab-bar-item {{selected === index ? 'active' : ''}}"
    data-detail="{{item}}"
    data-index="{{index}}"
    bindtap="switchTab">
    {{item.text}}
  </view>
</view>
